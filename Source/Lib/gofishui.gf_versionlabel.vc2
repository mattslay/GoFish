DEFINE CLASS gf_versionlabel AS label 
 	*< CLASSDATA: Baseclass="label" Timestamp="" Scale="Pixels" Uniqueid="" />

	*<PropValue>
		Alignment = 1
		Anchor = 8
		BackColor = 0,128,0
		BackStyle = 0
		Caption = "Version"
		FontBold = .F.
		FontItalic = .F.
